# Database
DATABASE_URL=postgresql://user:password@localhost:5432/medcenter
DATABASE_TEST_URL=postgresql://user:password@localhost:5432/medcenter_test

# Redis
REDIS_URL=redis://localhost:6379

# Security
SECRET_KEY=your-secret-key-here-change-in-production
